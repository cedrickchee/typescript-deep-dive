    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "client/build",
        // "outFile": "client/build/app.js",
        "downlevelIteration": true,
        "noLib": false
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "client/*.ts",
        "client/**/*.ts",
        "client/**/**/*.ts"
    ]
}